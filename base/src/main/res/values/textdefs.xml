<resources>


    <!--
    ========================================================
    Accent styles use regular ASCII letters but use unicode
    characters that add accents of other decorations around
    each character. Same accent will be applied to all letters.

    Character set styles replace regular ASCII table printing
    characters with unicode characters to create different
    styles; characters are defined for ASCII range:
    32 (space) - 126 (tilde).
    ========================================================
    -->

    <!-- follow this schema when defining accent character style! -->
    <!-- see existing arrays below for examples -->
    <integer name="AccentIdIndex">0</integer>
    <integer name="AccentNameIndex">1</integer>
    <integer name="AccentValue">2</integer>

    <!-- follow this schema when defining style character sets! -->
    <!-- see existing arrays below for examples -->
    <integer name="CharsetIdIndex">0</integer>
    <integer name="CharsetNameIndex">1</integer>
    <integer name="CharsetReverseIndex">2</integer>
    <integer name="CharsetIndex">3</integer>

