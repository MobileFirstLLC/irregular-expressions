<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="@dimen/kbd_bg_gap"
        android:left="@dimen/kbd_bg_gap"
        android:right="@dimen/kbd_bg_gap"
        android:top="@dimen/kbd_bg_gap">
        <shape>
            <stroke
                android:width="1dp"
                android:color="?attr/key_border_color" />
            <solid android:color="?attr/key_bg_pressed" />
            <padding
                android:bottom="1dp"
                android:left="1dp"
                android:right="1dp"
                android:top="1dp" />
            <corners
                android:bottomLeftRadius="4dp"
                android:bottomRightRadius="4dp"
                android:topLeftRadius="4dp"
                android:topRightRadius="4dp" />
        </shape>
    </item>
</layer-list>